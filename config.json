			"minute": 10,
			"second": 58,
			"weekday": 1,
			"dst": 0
		},
		"Steckdose04_NEU": {
			"protocol": [ "quigg_gt1000" ],
			"id": [{
				"id": 2,
				"unit": 0
			}],
			"state": "on"
		},
		"Steckdose03_NEU": {
			"protocol": [ "quigg_gt1000" ],
			"id": [{
				"id": 2,
				"unit": 1
			}],
			"state": "on"
		},
		"Steckdose02_NEU": {
			"protocol": [ "quigg_gt1000" ],
			"id": [{
				"id": 2,
